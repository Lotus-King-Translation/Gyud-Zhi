msgctxt "e0fb30f72d134d25946d9729c1329d62"
msgid ""
"རྒྱུད་ བཞི་  བདུད་རྩི་ སྙིང་པོ་ ཡན་ལག་ བརྒྱད་པ་ གསང་བ་ མན་ངག་ གི་ རྒྱུད་ "
"ཅེས་བྱ་བ་ བཞུགས་ སོ་"
msgstr ""

msgctxt "addf16c4e4b748cebce2ba166d7ea0ff"
msgid ""
"། ས་ མེད་ མི་ འགྲུབ་ ཆུ་ མེད་ སྡུད་ མི་ ནུས ། ། མེ་ མེད་ མི་ སྨིན་ རླུང་ "
"མེད་ འཕེལ་ མི་ འགྱུར ། ། ནམ་མཁའ་ མེད་པ␣ར་ འཕེལ་བ␣འི་ གོ་མི་ ཕྱེད །"
msgstr ""

msgctxt "be589923f27047518d26534ec9b77a2f"
msgid ""
"གསོ་བ་ རིག་པ␣འི་ བསྟན་བཅོས་ སྨན་བླ␣འི་ དགོངས་ རྒྱན་ རྒྱུད་ བཞི␣འི་ གསལ་བྱེད་"
" བཻདུརྱ་ སྔོན་པོ␣འི་ མལླིཀ་ ཞེས་ བྱ་བ་ བཞུགས་ སོ །།"
msgstr ""

msgctxt "b5a6bd8cd13f4a70989f9211263d2ad4"
msgid ""
"དེ␣འི་ ཚེ་ འབྱུང་བ་ ཆེན་པོ་ རྣམས་ ཀྱི་ ནང་ ནས་ ས␣འི་ རང་བཞིན་ མེད་ ན་ "
"འབྱུང་བ་ གཞན་ རྣམས་ ཡོད་ ཀྱང་ ལུས་ འདི་ཉིད་ མི་ འགྲུབ་ སྟེ །"
msgstr ""

msgctxt "3c771cd57e8b48a0b1d216370e7ff758"
msgid ""
"ལུས་ འདི་ སྲ་བ་ ལ་སོགས་པ␣འི་ རང་བཞིན་ ཕལ་ཆེར་ ནི་ ས་ལས་ གྲུབ་པ␣འི་ ཕྱིར །"
msgstr ""

msgctxt "cc213bdcee584542b16c6a310c745d1a"
msgid ""
"དེ་བཞིན་ དུ་ འབྱུང་བ་ ཆུ་ མེད་ ན་ གཞན་ རྣམས་ ཡོད་ ཀྱང་ གཅིག་ ཏུ་ བསྡུ་བ␣ར་ "
"མི་ ནུས་ ཏེ་ གོར་ གོར་ མེར་མེར་པོ་ ལ་སོགས་པ་ འབྱུང་བ␣ར་ མི་ འགྱུར་ རོ །"
msgstr ""

msgctxt "6e003d2c60f041c380803c74b5814385"
msgid ""
"། འབྱུང་བ་ གཞན་ རྣམས་ ཡོད་ ཀྱང་ མེ␣འི་ རང་བཞིན་ མེད་ ན་རིམ་པ་ བཞིན་ མི་ "
"སྨིན་ ཏེ །  རུལ་ ཞིང་ མྱགས་པ␣ར་ འགྱུར་བ␣ས་ སོ །"
msgstr ""

msgctxt "88d2cbb0dd5a4e63a6c74adb9035f8c3"
msgid ""
"། གཞན་ རྣམས་ ཡོད་ ཀྱང་ རླུང་ གི་ རང་བཞིན་ མེད་ ན་ ཆེས་ ཆེ␣ར་ འཕེལ་བ␣ར་ མི་ "
"འགྱུར་ ཏེ །  ལྕི་ ཞིང་ འཐས་པ␣ར་ གནས་པ␣འོ །"
msgstr ""

msgctxt "edcd9c37c0ca4cd8abe4c99566702009"
msgid ""
"རྒྱུད་ བཞི་  བདུད་རྩི་ སྙིང་པོ་ ཡན་ལག་ བརྒྱད་པ་ གསང་བ་ མན་ངག་ གི་ རྒྱུད་ "
"ཅེས་བྱ་བ་ བཞུགས་ སོ་ )"
msgstr ""

msgctxt "c4a3bd9795114780ae48c211709c916b"
msgid "། ས་ལས་ ཤ་ དང་རུས་པ་ སྣ་དྲི་ སྐྱེད །"
msgstr ""

msgctxt "aeb728b7948545e0b386ed4f20289ec4"
msgid "། མེ་ ལ␣ས་ དྲོད་ དང་ མདོག་ གསལ་མིག་ གཟུགས་ སྐྱེད །"
msgstr ""

msgctxt "0d0af7e5ea7e4edaad8e323fa24e0e2e"
msgid "། མཁའ་ ལ␣ས་ བུ་ག་ རྣ་བ་ སྒྲ་ སྐྱེད་ འགྱུར །"
msgstr ""

msgctxt "d92b657c40e04660b9ce8e7bf1457069"
msgid ""
"གསོ་བ་ རིག་པ␣འི་ བསྟན་བཅོས་ སྨན་བླ␣འི་ དགོངས་ རྒྱན་ རྒྱུད་ བཞི␣འི་ གསལ་བྱེད་"
" བཻདུརྱ་ སྔོན་པོ␣འི་ མལླིཀ་ ཞེས་ བྱ་བ་ བཞུགས་ སོ །།"
msgstr ""

msgctxt "e735b8109b4546a2a74d5d04a4a0235d"
msgid ""
"། མངལ་ དུ་ གནས་པ␣འི་ སེམས་ཅན་ རང་ གི་ སེམས་ལས་ མིག་ ལ་སོགས་པ␣འི་ དབང་པོ་ ལྔ་"
" དང་ རྣམ་པ␣ར་ ཤེས་པ་ ལྔ་པོ་ འབྱུང་ ཞིང་ སྐྱེ་བ་ ཡིན་ ནོ །"
msgstr ""

msgctxt "d83fcc71164349658ddc1146f95e61ef"
msgid ""
"། ཚུལ་ དེ་ དང་ མཐུན་པ␣ར་ འབྱུང་བ་ རྣམས་ ཀྱི་ ནང་ ནས་ ས␣འི་ རང་བཞིན་ གྱི་ "
"ཡོན་ཏན་ དེ་ལས་ ལུས་ ཀྱི་ ཤ་ དང་རུས་པ་ ལ་སོགས་ སྲ་བ␣འི་ ཆ་ཤས་ རྣམས་ འབྱུང་ "
"ཞིང་ ཁྱད་པར་ དུ་ སྣ␣འི་ དབང་པོ་ གཟུགས་ཅན་པ་ ཟངས་ ཁབ་ གཤིབས་པ་ ལྟ་བུ་ དང༌ །  "
"དེ␣འི་ ཡུལ་ དྲི␣འི་ རང་བཞིན་ རྣམས་ སྐྱེད་པ␣ར་ བྱེད་ དོ །"
msgstr ""

msgctxt "a57f40b1c1bf4fdc8bdecbd67951cc10"
msgid ""
"། ཆུ␣འི་ རང་བཞིན་ གྱི་ ཡོན་ཏན་ དེ་ལས་ ནི་ ཁྲག་ དང༌ ། ། ཁྱད་པར་ དུ་ ལྕེ␣འི་ "
"དབང་པོ་ གཟུགས་ཅན་པ་ ཟླ་བ་ བཀས་པ་ ལྟ་བུ་ དང༌ །  དེ␣འི་ ཡུལ་ རོ་དྲུག་ གི་ "
"རང་བཞིན་ ལ་སོགས་པ␣འི་ བརླན་ ཞིང་ གཤེར་བ␣འི་ རྣམ་པ་ རྣམས་ སྐྱེད་པ␣ར་ བྱེད་ དོ"
" །"
msgstr ""

msgctxt "64c4f8d2e90d48cfb05dfdc35c060337"
msgid ""
"། མེ␣འི་ རང་བཞིན་ གྱི་ ཡོན་ཏན་ དེ་ལས་ དྲོད་ ཀྱི་ ཆ་ཤས་ ཀུན་ དང་ ལུས་ ཀྱི་ "
"མདོག་ རབ་ཏུ་ གསལ་བ་ དག་ འབྱུང་ ཞིང༌ །  ཁྱད་པར་ དུ་ མིག་ གི་ དབང་པོ་ "
"གཟུགས་ཅན་པ་ ཟར་མ␣འི་ མེ་ཏོག་ ལྟ་བུ་ དང༌ །  དེ␣འི་ ཡུལ་ གཟུགས་ ཀྱི་ རང་བཞིན་ "
"རྣམས་ སྐྱེད་པ␣ར་ བྱེད་ དོ །"
msgstr ""

msgctxt "c37b2378631d42eabca26bb2f8a9bf06"
msgid ""
"། རླུང་ གི་ རང་བཞིན་ གྱི་ ཡོན་ཏན་ དེ་ལས་ ཕྱི་ ནང་ དུ་ རྒྱུ་པ␣འི་ དབུགས་ དང་ "
"དེ␣འི་ ཆ་ཤས་ རྣམས་ འབྱུང་ ཞིང༌ །  ཁྱད་པར་ དུ་ ལུས་ ཀྱི་ དབང་པོ་ གཟུགས་ཅན་པ་ "
"བྱ་ རེག་ འཇམ་ གྱི་ པགས་པ་ ལྟ་བུ་ ཡོངས་སུ་ འཛིན་པ␣འི་ ལུས་ ཀྱི་ པགས་པ་ དང༌ ། "
" དེ␣འི་ ཡུལ་ རེག་བྱ␣འི་ རང་བཞིན་ རྣམས་ སྐྱེད་པ␣ར་ བྱེད་ དོ །"
msgstr ""

msgctxt "3a1d4463e9c8487a8584ecfc12204ae7"
msgid ""
"། ནམ་མཁ␣འི་ རང་བཞིན་ གྱི་ ཡོན་ཏན་ དེ་ལས་ ལུས་ ཀྱི་ ཕྱི་ ནང་ སྲོག་ཟུངས་ ཀྱི་ "
"བུ་ག་ རྣམས་ དང༌ །  ཁྱད་པར་ རྣ་བ␣འི་ དབང་པོ་ གྲོ་ག␣འི་ འཛེར་བུ་ གཅུས་པ་ "
"ལྟ་བུ་ དང༌ །  དེ␣འི་ ཡུལ་ སྒྲ␣འི་ རང་བཞིན་ རྣམས་ འབྱུང་ ཞིང་ བསྐྱེད་པ␣ར་ "
"འགྱུར་བ་ ཡིན་ ནོ །"
msgstr ""

msgctxt "c2d592612a6a4884a284ef90d9340439"
msgid ""
"རྒྱུད་ བཞི་  བདུད་རྩི་ སྙིང་པོ་ ཡན་ལག་ བརྒྱད་པ་ གསང་བ་ མན་ངག་ གི་ རྒྱུད་ "
"ཅེས་བྱ་བ་ བཞུགས་ སོ་"
msgstr ""

msgctxt "01165f69a40c4db5a87d49449bd5ffc2"
msgid "། དབྱེ་བ་ རླུང་ དང་ མཁྲིས་པ་ བད་ཀན་ གསུམ །"
msgstr ""

msgctxt "6b86a71b5a464950a27fc22194e03a4e"
msgid ""
"། ཉེས་པ་ ནད་གཞི␣འི་ དབྱེ་བ་ ལ་ གཙོ་ ཆེ་བ␣འི་ དབང་ དུ་ བྱས་ ན །  རླུང་ དང་ "
"མཁྲིས་པ་ དང་ བད་ཀན་ ཏེ་ རྣམ་པ་ གསུམ་ དུ་ཡོད་ ལ །"
msgstr ""

msgctxt "7d77fd1b48cb4561a42dc136e3d5b6a9"
msgid "། རྣམ་པ␣ར་ མ་ གྱུར་ ལུས་ གནས་ གྱུར་པ␣ས་ འཇོམས །"
msgstr ""

msgctxt "166061de78d8487f87a525409047aa4d"
msgid ""
"གསོ་བ་ རིག་པ␣འི་ བསྟན་བཅོས་ སྨན་བླ␣འི་ དགོངས་ རྒྱན་ རྒྱུད་ བཞི␣འི་ གསལ་བྱེད་"
" བཻདུརྱ་ སྔོན་པོ␣འི་ མལླིཀ་ ཞེས་ བྱ་བ་ བཞུགས་ སོ །།"
msgstr ""

msgctxt "aebdd5cf761247fd9f3a301789462876"
msgid ""
"ནད་གཞི␣འི་ ཉེས་པ་ དེ་ གསུམ་པོ་ རང་རང་ གི་ གནས་ དང་ ལྡང་ཚད་ ལ␣ས་ རྣམ་པ␣ར་ མ་ "
"གྱུར་པ་ རང་སོར་ གནས་པ␣འི་ ཚེ་ ལུས་ འདི་ཉིད་ ནད་མེད་ ཐ་མལ་ དུ་ གནས་པ␣འི་ "
"གྲོགས་ བྱེད་ ལ །"
msgstr ""

msgctxt "87c490a0cc0a463eaf02a962223f7f4a"
msgid ""
"གལ་ཏེ་ རང་གནས་ དང་ ལྡང་ཚད་ ལ␣ས་ གཞན་ དུ་ གྱུར་ ན་ ལོག་པ␣འི་ ལ␣ས་ བྱས་ ནས་ "
"ལུས་ཟུངས་ སོགས་ གནོད་ བྱ་ རྣམས་ ལ་ འཚེ་བ␣ར་ བྱེད་པ␣ས་ ན་ ལུས་ འཇོམས་པ␣ར་ "
"འགྱུར་བ␣འི་ རྒྱུ་ ཡང་ ཡིན་ ནོ །"
msgstr ""

msgctxt "d0670366d4b246d78e5383887601c6a4"
msgid ""
"རྒྱུད་ བཞི་  བདུད་རྩི་ སྙིང་པོ་ ཡན་ལག་ བརྒྱད་པ་ གསང་བ་ མན་ངག་ གི་ རྒྱུད་ "
"ཅེས་བྱ་བ་ བཞུགས་ སོ་"
msgstr ""

msgctxt "4b7d4abb35d047259dc991939c3f8f1e"
msgid ""
"། རླུང་ ལ་ དབྱེ་ ན་ སྲོག་ འཛིན་ གྱེན་རྒྱུ་ དང༌ ། ། ཁྱབ་བྱེད་ མེ་ མཉམ་ ཐུར་ "
"སེལ་ རྣམ་པ་ ལྔ །"
msgstr ""

msgctxt "39bdfdc05dbb4c1da12c52a6201e527a"
msgid ""
"། ཉེས་པ␣འི་ ལ␣ས་ ནི་ རླུང་ གིས་ དབུགས་ འབྱིན་རྔུབ ། ། བསྐྱོད་ དང་ལ␣ས་ སྤྱོད་"
" ཤུགས་ འབྱིན་ གནོད་ བྱ་ རྒྱུ ། ། དབང་པོ་ གསལ་ དང་ ལུས་ རྗེས་འཛིན་པ␣ར་ བྱེད །"
msgstr ""

msgctxt "ba9264865e004caeb70b1942d258cf38"
msgid ""
"གསོ་བ་ རིག་པ␣འི་ བསྟན་བཅོས་ སྨན་བླ␣འི་ དགོངས་ རྒྱན་ རྒྱུད་ བཞི␣འི་ གསལ་བྱེད་"
" བཻདུརྱ་ སྔོན་པོ␣འི་ མལླིཀ་ ཞེས་ བྱ་བ་ བཞུགས་ སོ །།"
msgstr ""

msgctxt "038dcf0be0954a60b8b899d6ba2d8f13"
msgid ""
"དེ་ཡང་ རླུང་ ལ་ དབྱེ་ ན་ སྲོག་ འཛིན་ དང༌ །  གྱེན་རྒྱུ་ དང༌ །  ཁྱབ་བྱེད་ དང༌ "
"།  མེ་ མཉམ་ དང༌ །  ཐུར་ སེལ་ དང་ བཅས་པ་ སྟེ་ རྣམ་པ་ ལྔ␣འོ །"
msgstr ""

msgctxt "eb51d66969ba4ba3888ee7edd1cc04e5"
msgid ""
"དང་པོ་ རླུང་ གིས་ ནི་ དབུགས་ གྱེན་ དུ་ འབྱིན་པ་ དང༌ །  ནང་ དུ་ རྔུབ་པ་ དང༌ "
"།  ལུས་ ཀྱི་ འདུ་བྱེད་ བྱ་བ␣འི་ ཡུལ་ ལ་ བསྐྱོད་པ་ དང༌ །  ལུས་ངག་ ཡིད་ གསུམ་ "
"གྱི་ ལ␣ས་ ལ་ ལོངས་སྤྱོད་ ཅིང་ འཇུག་པ་ དང༌ །  བཤང་གཅི་ དང་ ངར་སྣབས་ "
"ལ་སོགས་པ␣འི་ ཤུགས་ འབྱིན་པ་ དང༌ །  དྭངས་མ་ ལ་སོགས་པ␣འི་ གནོད་ བྱ་ ལུས་ཟུངས་ "
"བདུན་པོ་ རང་རང་ གི་ བུ་ག་ རྣམས་ སུ་ བདེ་བ␣ར་ རྒྱུ་ ཞིང་ ཁྱབ་པ␣ར་ བྱེད་པ་ དང༌"
" །  མིག་ ལ་སོགས་པ␣འི་ དབང་པོ་ ལྔ␣འམ་ དྲུག་ རྣམས་ ཀྱང་ རང་རང་ གི་ ཡུལ་ གཟུགས་"
" སོགས་ ཉེ་བར་ འཛིན་པ་ གསལ་བ␣ར་ བྱེད་པ་ སྟེ །"
msgstr ""

msgctxt "bbdc154cfd8d46769bca7f5b6eb02e11"
msgid ""
"དེ་ རྣམས་ ཀྱི་ སྒོ་ ནས་ ལུས་ འདི་ཉིད་ ལ་ མི་ གནོད་ ཅིང་ རྗེས་སུ་ འཛིན་པ␣ར་ "
"བྱེད་ དོ །"
msgstr ""

msgctxt "0080b6c4cda6426bbdf3f901b617ef54"
msgid ""
"རྒྱུད་ བཞི་  བདུད་རྩི་ སྙིང་པོ་ ཡན་ལག་ བརྒྱད་པ་ གསང་བ་ མན་ངག་ གི་ རྒྱུད་ "
"ཅེས་བྱ་བ་ བཞུགས་ སོ་"
msgstr ""

msgctxt "2edd49b139f745cfb5aa285a93c37096"
msgid ""
"། ཁྱད་པར་ སྲོག་ འཛིན་ རླུང་ དེ་ སྤྱི་བོ␣ར་ གནས ། ། གྲེ་བ་ བྲང་གཞུང་ རྒྱུ་ "
"ཞིང་ ཟས་སྐོམ་ མིད ། ། དབུགས་ རྔུབ་ མཆིལ་ འདོར་ སྦྲིད་པ་ སྒྲེག་པ་ འབྱིན ། ། "
"བློ་ དང་ དབང་པོ་ གསལ་ ཞིང་ སེམས་ འཛིན་བྱེད །"
msgstr ""

msgctxt "49e37f2ff7874378b6569073d6707baf"
msgid ""
"། གྱེན་ དུ་ རྒྱུ་བ␣འི་ རླུང་ ནི་ བྲང་ ལ་ གནས ། ། སྣ་ ལྕེ་ ལྐོག་མ␣ར་ རྒྱུ་ "
"ཞིང་ ཚིག་ འབྱིན་ ལ ། ། སྟོབས་ མདངས་ ཁ་དོག་ འབད་རྩོལ་ དྲན་ གསལ་བྱེད །"
msgstr ""

msgctxt "274a305d37704d90b97afaf25cbdd4fe"
msgid ""
"གསོ་བ་ རིག་པ␣འི་ བསྟན་བཅོས་ སྨན་བླ␣འི་ དགོངས་ རྒྱན་ རྒྱུད་ བཞི␣འི་ གསལ་བྱེད་"
" བཻདུརྱ་ སྔོན་པོ␣འི་ མལླིཀ་ ཞེས་ བྱ་བ་ བཞུགས་ སོ །།"
msgstr ""

msgctxt "c1e6855628334657bb502438c2b00b33"
msgid ""
"དང་པོ་ སྲོག་ འཛིན་ ཞེས་ བྱ་བ␣འི་ རླུང་ དེ་ ནི་ གཙོ་བོ␣ར་ སྤྱི་བོ་ བདེ་ཆེན་ "
"གྱི་ འཁོར་ལོ␣ར་ གནས་པ་ སྲོག་ཆེན་པོ␣འི་ སྲོག་ འཛིན་ གྱི་ རླུང་ ཞེས་ བྱ་བ་ "
"དོན་ གཅིག་ ལ །"
msgstr ""

msgctxt "efccf0f2d36e48ff94ebf1724d5b7543"
msgid ""
"དེ་ཡང་ རླུང་ ཐམས་ཅད་ ཀྱི་ རྩ་བ་ ཡིན་པ␣ས་ ཀུན་གཞི␣འི་ རྣམ་པ␣ར་ ཤེས་པ་ "
"ཉོན་མོངས་པ␣འི་ ཡིད་ ལྷན་ཅིག་པ␣འི་ གཡོ་བ་ དེ་ ལ་ བྱ་ ཞིང༌ །"
msgstr ""

msgctxt "b3374301045c48208c58a7913668ba35"
msgid ""
"དེ་ ནི་ དོན་ ལ་རྩ་ དབུ་མ␣འི་ སུམ་ཆ་ ལྟེ་བ་ ནས་ སྤྱི་བོ␣འི་ བར་ ཐམས་ཅད་ "
"གང་བ␣འི་ ཚུལ་ གྱིས་ གནས་ ཀྱང༌ །"
msgstr ""

msgctxt "b4a152cfc91e45cd996c7c605331e4a5"
msgid ""
"ལ␣ས་ བྱེད་པ␣འི་ ཚུལ་ ནི་ གཙོ་བོ␣ར་ སྤྱི་བོ␣ར་ གནས་ ནས་ ཡུལ་ གྲེ་བ་ དང་ "
"བྲང་གཞུང་ དུ་ རྒྱུ་ ཞིང༌ །  ཟས་སྐོམ་ མིད་པ་ དང༌ །  དབུགས་ རྔུབ་པ་ དང༌ །  "
"མཆིལ་མ་ འདོར་བ་ དང༌ །  སྦྲིད་པ་ དང༌ །  སྒྲེག་པ་ འབྱིན་པ་ དང༌ །  བློ་གྲོས་ "
"དང་ མིག་ ལ་སོགས་པ␣འི་ དབང་པོ་ གསལ་ ཞིང་ སེམས་ མི་ གཏོང་བ␣ར་ རང་ དང་ ལྷན་ཅིག་"
" ཏུ་ འཛིན་པ␣ར་ བྱེད་པ་ ཡིན་ ནོ །"
msgstr ""

msgctxt "5ff15796a30345bfa9d73519951421ab"
msgid ""
"། གྱེན་ དུ་ རྒྱུ་བ་ ཞེས་ བྱ་བ␣འི་ རླུང་ ནི་ གཙོ་བོ␣ར་ བྲང་ ལ་ གནས་ བྱས་ ནས "
"།  ཡུལ་ སྣ་ ལྕེ་ ལྐོག་མ␣ར་ རྒྱུ་ ཞིང༌ །"
msgstr ""

msgctxt "716a481b50e44a00ba623e5f22cbf141"
msgid ""
"རྐང་ལག་ རྣམས་ འདེགས་པ་ དང་ འཇོག་པ␣ར་ བྱེད་པ␣འི་ ངང་ ནས་ འགྲོ་བ་ དང༌ །  "
"བརྐྱང་བསྐུམ་ དང༌ །  ཁ་ མིག་ སོགས་ འབྱེད་ འཛུམ་ ལ་སོགས་ ཏེ །"
msgstr ""

msgctxt "6a00569adedd454a937b0fe64b2397b7"
msgid ""
"། ཕོ་བ␣འི་ མེ་ དང་ མཉམ་ དུ་ འཇུག་པ␣འི་ རླུང་ ནི་ གཙོ་བོ␣ར་ ཕོ་བ␣ར་ གནས་ ནས་ "
"ཡུལ་ རྒྱུ་མ་ ལ་སོགས་པ␣འི་ ནང་ཁྲོལ་ ཀུན་ ཏུ་ རྒྱུ་ ཞིང༌ །"
msgstr ""

msgctxt "edd1294a44af4398b7af0a9d59cfc726"
msgid ""
"ལ␣ས་ ཁ་ཟས་ རྣམས་ འཇུ་བ །  དྭངས་སྙིགས་ འབྱེད་ ཅིང་ ཁྲག་ ལ་སོགས་པ␣འི་ གནོད་ "
"བྱ་ གཞན་ རྣམས་ སྨིན་པ␣ར་ བྱེད་པ་ ཡིན་ ནོ །"
msgstr ""

msgctxt "de75c51a63904f4e8761a5b7560b493d"
msgid ""
"། ཐུར་ དུ་ སེལ་བ␣འི་ རླུང་ ནི་ གཙོ་བོ␣ར་ གཞང་ ལ་ གནས་ ནས །  ཡུལ་ ལོང་ "
"སྟོད་སྨད་ དང༌ །  ལྒང་པ་ དང༌ །  གསང་བ␣འི་ གནས་ དང་ བཅས་པ␣འི་ བརླ␣འི་ ནང་ དུ་ "
"རྒྱུ་ ཞིང༌ །"
msgstr ""

msgctxt "1699fc32c2a846d192695a0a478a862a"
msgid ""
"ལ␣ས་ ཁུ་ཁྲག་ དང༌ །  བཤང་གཅི་ རྣམས་ དང༌ །  མངལ་ ནང་ ན་ གནས་པ␣འི་ བུ་ "
"ལ་སོགས་པ་ འབྱིན་པ་ དང༌ །  སྡོམ་པ␣འི་ བྱ་བ་ རྟག་ ཏུ་ བྱེད་པ་ ཡིན་ ནོ །"
msgstr ""

msgctxt "0105833b4435495dbe8a3014a0d905d2"
msgid ""
"རྒྱུད་ བཞི་  བདུད་རྩི་ སྙིང་པོ་ ཡན་ལག་ བརྒྱད་པ་ གསང་བ་ མན་ངག་ གི་ རྒྱུད་ "
"ཅེས་བྱ་བ་ བཞུགས་ སོ་"
msgstr ""

msgctxt "d91c0b9ce5004ef9931260719e9bb50f"
msgid "། རླུང་ གི་ མཚན་ཉིད་ རྩུབ་ ཅིང་ ཡང་བ་ དང༌ །"
msgstr ""

msgctxt "504c2f788f6c42b492f4151db9596ffc"
msgid ""
"དེ␣འི་ རྣམ་པ␣ར་ མ་ གྱུར་པ␣འི་ རླུང་ གི་ མཚན་ཉིད་ ནི་ རང་རང་ གི་ ངོ་བོ་ "
"འཇམ་པ་ དང་ ལྕི་བ་ མ་ཡིན་པ␣འི་ རྩུབ་ ཅིང་ ཡང་བ་ དང༌ །  དྲོ་བ་ སྦོམ་པ་ དང༌ །  "
"སྙི་བ་ དང་ བརྟན་པ␣འི་ ངོ་བོ་ དང་ བྲལ་བ␣ས་ རང་ གི་ རང་བཞིན་ གྲང་ ཞིང་ "
"ཕྲ་མོ␣ར་ གནས་ ལ་ སྲ་བ་ སྟེ་ མཁྲེགས་ ཤིང་ གཡོ་བ␣འི་ ངོ་བོ␣ར་ གནས་པ་ ཡིན་ ནོ །"
msgstr ""

msgctxt "291630cee6764d84a30f47dfe2e2f6e5"
msgid ""
"རྒྱུད་ བཞི་  བདུད་རྩི་ སྙིང་པོ་ ཡན་ལག་ བརྒྱད་པ་ གསང་བ་ མན་ངག་ གི་ རྒྱུད་ "
"ཅེས་བྱ་བ་ བཞུགས་ སོ་"
msgstr ""

msgctxt "c12801c2b9424fd39e3162ed1f9d4f2d"
msgid ""
"། དང་པོ་ ནད་ འཁྲུགས་ སྣ་ཚོགས་ ཚུལ་ ཅན་ གྱིས ། ། གྲངས་མེད་ གྱུར་པ␣ས་ ལུས་ "
"ཉིད་ གཟི␣ར་ གྱུར་པ །"
msgstr ""

msgctxt "ea4fd7ff487a4638b7a6dcfa1f49cd48"
msgid ""
"། དེ་ རྒྱུ་ རེ་རེ␣ར་ བསྟན་པ␣ར་ མི་ ནུས་པ␣ས ། ། དེ་བ␣ས་ ནད་ ཀུན་ སྤྱི་ རྒྱུ་ "
"བརྗོད་པ་ ནི ། ། ནད་ རྣམས་ ཀུན་ གྱི་ རྒྱུ་ ནི་ གཅིག་པུ་ སྟེ ། ། བདག་མེད་ དོན་"
" མ་ རྟོགས་པ␣ས་ མ་རིག་ ཅེས །"
msgstr ""

msgctxt "0b75e00c82d94336975eb2c5e7da9e63"
msgid ""
"། དཔེར་ན་ བྱ་ ནི་ མཁའ་ ལ་ ལྡིང་ གྱུར་ ཀྱང༌ ། ། རང་ གི་ གྲིབ་མ་ དེ་ དང་ མི་ "
"འབྲལ་ ཞིང༌ ། ། འགྲོ་ ཀུན་ བདེ་བ␣ར་ གནས་ ཤིང་ སྤྱོད་ ན་ ཡང༌ ། ། མ་རིག་ "
"ལྡན་པ␣ས་ ནད་ དང་ འབྲལ་ མི་ སྲིད །"
msgstr ""

msgctxt "2069c3eb286c4ba984102ff4e236761b"
msgid ""
"། ཁྱད་པར་ རྒྱུ་ ནི་ མ་རིག་ ལ␣ས་ བྱུང་བ␣འི ། ། འདོད་ཆགས་ ཞེ་སྡང་ གཏི་མུག་ "
"དུག་ གསུམ་ ལ␣ས ། ། འབྲས་བུ་ རླུང་ མཁྲིས་ བད་ཀན་ ཉེས་པ་ སྐྱེད །"
msgstr ""

msgctxt "4f8a3aed8e7a4b3db7f2af3126a7d9fa"
msgid ""
"རྒྱུད་ བཞི་  བདུད་རྩི་ སྙིང་པོ་ ཡན་ལག་ བརྒྱད་པ་ གསང་བ་ མན་ངག་ གི་ རྒྱུད་ "
"ཅེས་བྱ་བ་ བཞུགས་ སོ་"
msgstr ""

msgctxt "934ad7b3a76545e5aa702f5c22f65b4a"
msgid "། རླུང་ ནི་ ཚ་གྲང་ གཉིས་ཀ␣འི་ ཁྱབ་བྱེད་ དེ །"
msgstr ""

msgctxt "0483ecae858d4cc79758afbdc7efa45c"
msgid ""
"། སྟོད་སྨད་ ཕྱི་ ནང་མ་ ལུས་ ཁྱབ་པ␣ར་ རྒྱུ ། ། ཚ་གྲང་ གཉིས་ཀ་ འཁྲུག་ ཅིང་ "
"སློང་བ␣ར་ བྱེད །"
msgstr ""

msgctxt "53ba83b904b6439b9879b4c7519c1265"
msgid ""
"གསོ་བ་ རིག་པ␣འི་ བསྟན་བཅོས་ སྨན་བླ␣འི་ དགོངས་ རྒྱན་ རྒྱུད་ བཞི␣འི་ གསལ་བྱེད་"
" བཻདུརྱ་ སྔོན་པོ␣འི་ མལླིཀ་ ཞེས་ བྱ་བ་ བཞུགས་ སོ །།"
msgstr ""

msgctxt "fd07eff05f444c79a1cc73393b6d02d2"
msgid ""
"། དེ་ལྟར་ མཁྲིས་པ་ དང་ བད་ཀན་ གཉིས་ ཚ་གྲང་ གི་ ནད་ སོ་སོ␣འི་ རྒྱུ␣འི་ "
"གཙོ་བོ་ ཡིན་ ལ །"
msgstr ""

msgctxt "43e561c0610c4dcdbac1f90c7b27ea3e"
msgid ""
"མདོར་ན་ ལུས་ ཀྱི་ སྟོད་སྨད་ ཕྱི་ ནང་མ་ ལུས་པ་ ཀུན་ ཏུ་ ཁྱབ་པ␣ར་ རྒྱུ་ ཞིང་ "
"ཚ་གྲང་ གཉིས་ཀ་ རང་བཞིན་ དུ་ གནས་པ␣འི་ དབང་མེད་པ␣ར་ འཁྲུགས་ ཤིང་ སློང་བ␣ར་ "
"བྱེད་པ་ སྟེ །"
msgstr ""

msgctxt "3aedbd3862a24998b4f34da1aec760b9"
msgid ""
"དེ་བས་ན་ རླུང་ ཞེས་ བྱ་བ␣འི་ ཉེས་པ་ འདི་ ནི་ ཚ་གྲང་ གཉིས་ ལ␣ས་ གྱེས་པ␣འི་ "
"ནད་ ཀྱི་ རྣམ་ གྲངས་ མང་པོ␣ར་ བཤད་པ་ ཀུན་ གྱི་ ནང་ཚན་ མ་ལུས་པ་ མེད་པ་ ཐམས་ཅད་"
" ཀྱི་ རྒྱུ␣འི་ གཙོ་བོ་ གཅིག་པུ་ ཉིད་ ཡིན་ ནོ་ ཞེས་ གསུངས་ སོ །"
msgstr ""

msgctxt "928418043ce04410893cb592844263d0"
msgid ""
"རྒྱུད་ བཞི་  བདུད་རྩི་ སྙིང་པོ་ ཡན་ལག་ བརྒྱད་པ་ གསང་བ་ མན་ངག་ གི་ རྒྱུད་ "
"ཅེས་བྱ་བ་ བཞུགས་ སོ་"
msgstr ""

msgctxt "67cb778ddac94e7bbfe5486a5cb22480"
msgid ""
"། ཁྱད་པར་ ཁ་ དང་ ཡང་ རྩུབ་ བསྟེན་ དྲགས་ དང༌ ། ། ཆགས་པ␣ས་ དུབ་ དང་ ལྟོ་ ཆག་ "
"གཉིད་ ཆག་ དང༌ ། ། ལྟོ་ སྟོང་ ལུས་ངག་ བྱ་བ་ དྲག་ཤུལ་ དང༌ ། ། ཁྲག་ མང་ ཟགས་ "
"དང་ འཁྲུ་སྐྱུག་ དྲག་པོ␣ས་ བཏབ ། ། བསེར་བུ␣ས་ བུས་ དང་ག་ ཆད་ ངུས་པ་ དང༌ ། ། "
"མྱ་ངན་ སེམས་ལས་ ཁ་ལས་ ཆེས་པ་ དང༌ ། ། བཅུད་ མེད་ ཁ་ཟས་ ཡན་ ལ་ གཏད་པ་ དང༌ ། ། "
"ཤུགས་ བཀག་པ་ དང་ ནན་ གྱིས་ བཙིར་བ་ ཡི ། ། རྐྱེན་ དེ་ རྣམས་ ཀྱིས་ རླུང་ནད་ "
"ལྡང་བ␣ར་ བྱེད །"
msgstr ""

msgctxt "52267c40914b4296989bf689e3e2c243"
msgid ""
"། རླུང་ གི་ ནད་ སློང་བ␣འི་ རྐྱེན་ ཁྱད་པར་བ་ ནི །  སྨན་ དང་ བཟའ་བཏུང་ གི་ "
"རིགས་ རོ་ཁ་བ་ དང༌ །  ཨ་རུ་བ་ རུ་ ལ␣ས་ གཞན་པ␣འི་ རོ་ བསྐ་བ␣འི་ རིགས་ དང༌ །  "
"རོ་ ཇི་ལྟར་ གྱུར་ ཀྱང་ ཡོན་ཏན་ ཡང་ ཞིང་ རྩུབ་པ་ དང༌ །  ཤུགས་ ལ␣ས་ སྲ་ ཕྲ་ "
"སྐྱ་ གཡོ་ གྲང་བ་ དང་ ལྡན་པ་ བསྟེན་ དྲགས་པ་ དང༌ །  བུད་མེད་ ལ་ ཆགས་པ་ མང་ དུ་"
" སྤྱད་པ␣ས་ དུབ་པ་ དང༌ །  ལན་ མང་པོ␣འི་ བར་ དུ་ ལྟོ་ ཆག་པ་ དང༌ །  ཟས་ ཀྱང་ "
"ཉུང་ཟད་ ཁོ་ན་ ལས་མ་ ཟོས་པ་ དང༌ །  ནུབ་ མང་པོ␣ར་ གཉིད་ ཆག་པ་ དང༌ །  སྔ་དྲོ་ "
"ལྟོ་ སྟོང་ ལ་ ལུས་ངག་ གི་ བྱ་བ་ དྲག་ཤུལ་ ཆེས་པ་ དང༌ །  གཏར་བ་ དང་ རྨས་པ་ "
"ལ་སོགས་པ␣འམ་ བུ་ག་ རྣམས་ ལ␣ས་ ཁྲག་ མང་ དུ་ ཟགས་པ་ དང༌ །  ཡང་ན་ འཁྲུ་སྐྱུག་ "
"དྲག་པོ␣ས་ བཏབ་པ␣འམ །  ག་བུར་ ལ་སོགས་པ␣འི་ དྲི་ དང་ ལྡན་ ནམ་ མེད་ ཀྱང་ རུང་ "
"སྟེ་ བསེར་བུ␣ས་ ཡུན་རིང་ དུ་ བུས་པ་ དང༌ །  ག་ཆད་པ་ སྟེ་ ཡི་ ཆད་ ཆད་ ཀྱི་ བར་"
" དུ་ ངུས་པ་ དང་ རང་ དང་ གཞན་ གྱི་ སྒོ་ ནས་ མྱ་ངན་ དང་སེམས་ ཀྱི་ ལ␣ས་ ཆེས་ "
"ཆེ␣ར་ བྱེད་པ་ དང༌ །  ཉིན་མཚན་ ཐོག་ཐག་ ཏུ་ སྨྲ་བ་ སོགས་ ཁ␣འི་ ལ␣ས་ ཆེས་པ་ དང༌"
" །  འབྲས་བུ་ ལ་སོགས་པ་ མ་ སྨིན་པ␣འམ་ ཐལ་ ཆེས་པ་ ལྟ་བུ་ བཅུད་ མེད་པ␣འི་ ཁ་ཟས་"
" ཁོ་ན་ ཡེན་ ལ་ གཏད་པ་ སྟེ་ རྒྱུན་ བསྲིང་བ་ དང༌ །  བཤང་གཅི་ ལ་སོགས་པ་ "
"འབྱུང་བ␣འི་ ཤུགས་ བཀག་པ །"
msgstr ""

msgctxt "08b7e4c42d924c2abcd6b20b032da96a"
msgid ""
"རྒྱུད་ བཞི་  བདུད་རྩི་ སྙིང་པོ་ ཡན་ལག་ བརྒྱད་པ་ གསང་བ་ མན་ངག་ གི་ རྒྱུད་ "
"ཅེས་བྱ་བ་ བཞུགས་ སོ་"
msgstr ""

msgctxt "9f4f6a981f064a1ba7a4142bcd71f471"
msgid "། དེ་ ལ་ རླུང་ ནི་ ནད་ ཀུན་ འཁྲུག་པ␣འི་ རྒྱུ །"
msgstr ""

msgctxt "5ac63efcd2ff4d7abe5376964fa00fad"
msgid "། སྣ་ འདྲེན་ མཇུག་ སྡུད་ འཐོར་ དང་ ཁྱབ་པ␣ར་ བྱེད །"
msgstr ""

msgctxt "10a3d31047314c99b99d2bc4b132894c"
msgid ""
"། རང་རྒྱུད་ གདུག་ ཅིང་ ནད་ མང་ རྩུབ་པ␣འི་ ཕྱིར ། ། ཐོག་མ␣ར་ རླུང་ གི་ "
"བཅོས་ཐབས་ བཤད་པ␣ར་ བྱ །"
msgstr ""

msgctxt "53c94072a6054794ae116204f7135ebe"
msgid "གསོ་རིག་ རྒྱུད་ བཞི␣འི་ འགྲེལ་ཆེན་ དྲང་སྲོང་ ཞལ་ལུང་ །"
msgstr ""

msgctxt "8094868a22db4464923a9787c3580c12"
msgid ""
"ཉེས་པ་ གསུམ་ ལ␣ས་ རླུང་ གི་ ནད་ ནི་ མཁྲིས་པ་ སོགས་ ནད་ གཞན་ ཀུན་ འཁྲུགས་པ␣ར་"
" བྱེད་པ␣འི་ རྒྱུ་ ཡིན་ ཏེ །"
msgstr ""

msgctxt "d12f0f9f45f54db48b40df64cc5db8cb"
