msgctxt "6e50b3cfc2a447fa8d8840c2809bb534"
msgid "ཚད་ གཞུག་ རླུང་ གིས་ འབུད་པ་ ལྟ་བུ་ ནད་ ཀྱི་ མཇུག་ སྡུད་པ␣ར་ བྱེད་པ །"
msgstr ""

msgctxt "f43f0e8393934c6db9c9ce89d60f1de5"
msgid ""
"གཡོ་བ␣འི་ རྐྱེན་ ལ␣ས་ ནད་ གང་ཡང་ གནས་ གཞན་ དུ་ བྱེར་བ␣ར་ བྱེད་པ␣ས་ འཐོར་བ ། "
" ཚ་གྲང་ ཀུན་ ལ་ ཁྱབ་པ␣ར་ བྱེད་པ །"
msgstr ""

msgctxt "f0174e189ed44bafae506b55247337a0"
msgid ""
"རང་བཞིན་ ནམ་ རང་ གི་ རྒྱུད་ གཞན་ ལ་ གནོད་པ␣འི་ འཕྲུལ་ རྩོམ་པ␣ས་ གདུག་པ །  "
"རང་ ངི་ ངོ་བོ་ འཇམ་པོ་ མིན་པ་ རྩུབ་པ␣ས་ ན་ རྩུབ་པ །"
msgstr ""

msgctxt "9c36bf3368234c78b06e2d0e9c335856"
msgid ""
"རླུང་ མཁྲིས་ བད་ཀན་ གསུམ་ གྱི་ ནང་ ནས་ རླུང་ གི་ ནད་ མང་བ␣ས་ ན་ ནད་ སྣ་མང་བ་"
" བཅས་ ཀྱི་ ཕྱིར་ རྒྱུད་ འདི␣ར་ ཐོག་མ␣ར་ རླུང་ གི་ ནད་ ཀྱི་ བཅོས་ཐབས་ བསྟན་པ་"
" ཡིན་ ནོ །"
msgstr ""

msgctxt "8d03b8730ecc49b3a97ff0b03d9c84be"
msgid ""
"རྒྱུད་ བཞི་  བདུད་རྩི་ སྙིང་པོ་ ཡན་ལག་ བརྒྱད་པ་ གསང་བ་ མན་ངག་ གི་ རྒྱུད་ "
"ཅེས་བྱ་བ་ བཞུགས་ སོ་"
msgstr ""

msgctxt "6fe60e252cfa45a585f8b5aedb339506"
msgid ""
"། ཟླ་བ་ གཉིས་པ་ བདུན་ ཕྲག་ ལྔ་པ་ ལ ། ། མངལ་ གནས་ ལུས་ ཀྱི་ དང་པོ་ ལྟེ་བ་ ཆགས"
" །"
msgstr ""

msgctxt "c0dc81dca49d49a69e18518c6ba13c0f"
msgid "། དྲུག་པ་ ལྟེ་བ་ ལ་ བརྟེན་ སྲོག་རྩ་ ཆགས །"
msgstr ""

msgctxt "a2aec5e57de04adf9976a581e171b0ca"
msgid "གསོ་རིག་ རྒྱུད་ བཞི␣འི་ འགྲེལ་ཆེན་ དྲང་སྲོང་ ཞལ་ལུང་"
msgstr ""

msgctxt "dbf22c18866f4a2b9f2c43de654814a8"
msgid ""
"ཟླ་བ་ གཉིས་པ་ ལ་ སྔ་མ␣འི་ ཞག་ ལྷག་མ་ དང་ བཅས་པ་ བསྣན་པ␣ས་ བདུན་ ཕྲག་ ལྔ་ "
"ཡོད་པ་ དག་ གོང་ གི་ སྟེང་ དུ་ བརྩེགས་པ␣འི་ བདུན་ ཕྲག་ ལྔ་པ␣འི་ སྐབས་ སུ་ "
"ཡང་དག་པ␣ར་ སྡུད་པ␣འི་ རླུང་ ཞེས་པ␣འི་ བྱེད་ལས་ ཀྱིས་ མངལ་ གནས་ དེ་ཉིད་ སྔར་ "
"གྱི་ གོར་ གོར་ མེར་མེར་ སོགས་ གང་རུང་ གི་ རྣམ་པ་ དེ་ལས་ འདས་ ནས་ མཁྲེགས་ "
"ཤིང་ མཁྲང་འགྱུར་ གྱི་ ཞག་ ཅེས་ བྱ་ ཞིང་ །"
msgstr ""

msgctxt "f8aafcc5ca114614bec6e54e917df997"
msgid ""
"དེ་ལྟར་ མཁྲང་འགྱུར་ གྱི་ ཞག་ འདི␣འི་ སྐབས་ སུ་ མངལ་ གནས་ ཀྱི་ སེམས་ཅན་ "
"དེ␣འི་ ལུས་ ཀྱི་ ཆ་ཤས་ ཀྱི་ ནང་ ནས་ དང་པོ་ ཁོ་ན␣ར་ རྩ་ རླུང་ ཐམས་ཅད་ "
"སྐྱེད་པ␣འི་ རྩ་བ་ ལྟ་བུ␣ར་ གྱུར་པ␣འི་ རྩ་ འདབ་ ཀྱི་ རྣམ་པ་ དེ་ཉིད་ ཆགས་ "
"འགྱུར་ གྱི་ ཚིགས་པ་ བཅུ་དྲུག་པ␣འི་ ཐད་ དུ་ ཆགས་པ␣ར་ འགྱུར་བ་ དང་ །"
msgstr ""

msgctxt "28948520f5d2434ba8f6e31574caec53"
msgid ""
"བདུན་ ཕྲག་ དྲུག་པ␣ར་ ཟས་ ཞེས་པ␣འི་ རླུང་ གི་ བྱེད་ལས་ ཀྱིས་ ལྟེ་བ␣འི་ རྩ་ "
"འདབ་ ཀྱི་ དབུས་ དེ་ ལ་ བརྟེན་ ནས་ སྲོག་པ་ རྩ་ ཞེས་ འཕར་རྩ་ ཐམས་ཅད་ ཀྱི་ "
"སྡོང་པོ་ སྲོག་ གི་ རླུང་ གི་ རྟེན་ དེ་ ནི་ དུས་ ཀྱི་ འཁོར་ལོ་ དང་ ཟབ་མོ་ "
"ནང་དོན་ གྱི་ དགོངས་པ་ ལྟར་ །"
msgstr ""

msgctxt "f2e845d79700423a9e99a8c66bfb05d6"
msgid ""
"འདི␣ར་ ནི་ འཁོར་ལོ་ བཞི་ དང་ ཤུགས་བསྟན་ བཅས་ ལྔ་ ལ␣ས་ མེད་ ཕྱིར་ མངལ་ གནས་ "
"རང་ཉིད་ ཀྱི་ རང་སོར་ བཅུ་དྲུག་ བར་ གྱེན་ ཕྱོགས་ སུ་ གྱེས་པ་ དང་ འབྲེལ །  "
"དེ␣འི་ ཡར་སྣེ་ ཚིགས་པ་ བརྒྱད་པ␣འི་ ཐད་ དུ་ སྙིང་ཁམས་ ཀྱི་ རྩ་ འདབ་མ་ "
"བཞི་པ␣འི་ རྣམ་པ་ དང་ བཅས་པ་ ཆགས་པ་ དང་ །"
msgstr ""

msgctxt "c98017dc06524dc8a9c802d821d863d0"
msgid ""
"བདུན་ ཕྲག་ བདུན་པ␣ར་ འཁྱིལ་ བར་ བྱེད་པ་ ཞེས་པ␣འི་ རླུང་ གི་ བྱེད་ལས་ ཀྱིས་ "
"སྙིང་ག་ ནས་ སྲོག་པ་ རྩ་ དེ་ རང་སོར་ བཅུ་བཞི་ ཚད་ ཙམ་ གྱེན་ དུ་ སོང་ སྟེ་ "
"ཨན་སྟོང་ ཚིགས་པ་ དང་པོ␣འི་ ཐད་ དུ་ མགྲིན་པ␣འི་ རྩ་འཁོར་ ཆགས་པ་ འདི␣ར་ དངོས་ "
"སུམ་ བསྟན་ ཀྱང་ ཤུགས་བསྟན་ དུ་ གོ་ དགོས་པ་ དང་ །"
msgstr ""

msgctxt "0baa07f728df4b5487028ef5a57a7ab3"
msgid ""
"དེ་ནས་ སླར་ཡང་ རླུང་ དེ␣འི་ བྱེད་ལས་ ཀྱིས་ གྱེན་ ཕྱོགས་ སུ་ རང་སོར་ "
"བཅུ་དྲུག་ བར་ གྱེས་ ཏེ་ ཡར་སྣེ་ སྤྱི་བོ␣འམ་ མགོ་བོ␣འི་ རྩ་ འདབ་ མིག་ གི་ "
"དབང་པོ་ དང་ བཅས་པ␣འི་ གཟུགས་དབྱིབས་ དོད་པ་ དང་ །"
msgstr ""

msgctxt "0c518e9e56da463ea534802a0d264501"
msgid ""
"དེ་ དང་ དུས་མཉམ་ དུ་ ལྟེ་བ་ ནས་ སྲོག་པ་ རྩ་ དེ་ ཐུར་ དུ་ རང་སོར་ བཅུ་བཞི་ "
"བར་ གྱེས་ ཏེ་ མར་སྣེ་ གསང་གནས་ ཀྱི་ རྩ་ འདབ་ དང་ བཅས་པ་ ཆགས་པ␣ར་ འགྱུར་བ་ "
"དང་ །"
msgstr ""

msgctxt "f96a73458b3140bf96d2f33c685c9bd6"
msgid ""
"བདུན་ ཕྲག་ བརྒྱད་པ␣ར་ བཟློག་ ཅིང་ བསྒྱུར་བ␣ར་ བྱེད་པ་ ཞེས་པ␣འི་ རླུང་ གི་ "
"བྱེད་ལས་ ཀྱིས་ མགོ་བོ␣འི་ རྩ་ འདབ་ མིག་ གི་ དབང་པོ་ དང་ བཅས་པ་ ཆགས་ ཤིང་ །"
msgstr ""

msgctxt "fc8203d0b4a84a0aa74fd4bd5a2b182b"
msgid ""
"རྒྱུད་ བཞི་  བདུད་རྩི་ སྙིང་པོ་ ཡན་ལག་ བརྒྱད་པ་ གསང་བ་ མན་ངག་ གི་ རྒྱུད་ "
"ཅེས་བྱ་བ་ བཞུགས་ སོ་"
msgstr ""

msgctxt "637ef2fbf5da4b7daa73351740967b6c"
msgid ""
"། འབྲེལ་པ་ རྩ་ ཡི་ གནས་ལུགས་ བསྟན་པ་ ནི ། ། ཆགས་ སྲིད་ འབྲེལ་པ་ ཚེ་ ཡི་ རྩ་ "
"དང་ བཞི །"
msgstr ""

msgctxt "fe5fdf5810ac48be838c340e4f2b980c"
msgid ""
"རྒྱུད་ བཞི་  བདུད་རྩི་ སྙིང་པོ་ ཡན་ལག་ བརྒྱད་པ་ གསང་བ་ མན་ངག་ གི་ རྒྱུད་ "
"ཅེས་བྱ་བ་ བཞུགས་ སོ་"
msgstr ""

msgctxt "7269238a5f7c466e90c2c12a8a6bd060"
msgid "། ཆགས་པ␣འི་ རྩ་ ནི་ ལྟེ་བ་ ལ␣ས་ གསུམ་ གྱེས །"
msgstr ""

msgctxt "468a882455df492a9ff4a1079cd9af86"
msgid "། རྩ་ གཅིག་ གྱེན་ ལ་ སོང་བ␣ས་ ཀླད་པ་ ཆགས །"
msgstr ""

msgctxt "8b91f9a9d2a543e08235e81bc2d8de5e"
msgid "། གཏི་མུག་ ཀླད་པ␣ར་ བརྟེན་ ནས་ གནས་པ་ སྟེ །"
msgstr ""

msgctxt "c05bf8fd873042039862f26adf7f17a2"
msgid "། དེ་ལས་ བད་ཀན་ སྐྱེད་པ␣ས་ སྟོད་ ན་ གནས །"
msgstr ""

msgctxt "6050e5534e0b4a399021c0a76c98b332"
msgid ""
"། ཞེ་སྡང་ སྲོག་རྩ་ ཁྲག་ ལ་ བརྟེན་ ནས་ གནས ། ། དེ་ལས་ མཁྲིས་པ་ སྐྱེད་པ␣ས་ བར་"
" ན་ གནས །"
msgstr ""

msgctxt "cdc03b573cad4240a1a3fab3b50c9c92"
msgid "། རྩ་ གཅིག་ ཐུར་ དུ་ ཟུག་པ␣ས་ གསང་བ་ ཆགས །"
msgstr ""

msgctxt "497bae18c4be4ea4bf79f2fd919c272e"
msgid ""
"། འདོད་ཆགས་ ཕོ་མོ␣འི་ གསང་བ␣ར་ གནས་པ་ སྟེ ། ། དེ་ལས་ རླུང་ སྐྱེད་ སྨད་ ན་ "
"གནས་པ་ ཡིན །"
msgstr ""

msgctxt "c0a93f41c1b3483a9864db0345b95a42"
msgid "གསོ་རིག་ རྒྱུད་ བཞི␣འི་ འགྲེལ་ཆེན་ དྲང་སྲོང་ ཞལ་ལུང་ །"
msgstr ""

msgctxt "c88225455ff84b6184bfe49324c40726"
msgid ""
"ལུས་ འདི་ཉིད་ མ␣འི་ མངལ་ དུ་ ཆགས་པ␣འི་ ཐོག་མ་ དེ␣ར་ ཆགས་ ཤིང་འཕེལ་བ␣འི་ རྩ་ "
"ལྟེ་བ␣འི་ གཟུགས་ དོད་པ་ དང་ སྦྲགས་ རྩ་ དེ␣འང་ ལྟེ་བ␣འི་ དབུས་ ནས་ རྩེ་མོ་ "
"མི་ འདྲ་བ་ གསུམ་ དུ་ གྱེས་པ་ སྟེ །"
msgstr ""

msgctxt "39fc34bdc2a04fb6876bce7419d9b649"
msgid ""
"ཆུ␣འམ་ ཟླ་བ␣འི་ ཁམས་ ཤས་ ཆེ␣ར་ རྒྱུ་བ␣འི་ རྩ་ གཅིག་ གྱེན་ ལ་ སོང་བ␣ས་ གཞི་ "
"དེ་ ལ་ བརྟེན་ ནས་ ཀླད་པ་ གཙོ་བོ␣ར་ གྱུར་པ␣འི་ མགོ་བོ␣འི་ ཆ་ཤས་ རྣམས་ ཆགས་ "
"ཤིང་ གྲུབ་པ་ ཡིན །  ཉོན་མོངས་ གཏི་མུག་ ཀྱང་ ཀླད་པ་ དེ་ ལ་ བརྟེན་ ནས་ གནས་པ་ "
"དང་ །"
msgstr ""

msgctxt "096f6da70a3848679eb080402ef8f8e4"
msgid ""
"དེ་ལས་ ལུས་ ཀྱི་ གྲང་བ␣འི་ རང་བཞིན་ བད་ཀན་ སྐྱེད་པ་ སྟོད་ ན་ གནས་པ␣ས་ "
"རྒྱུ་འབྲས་ རྟེན་ བཅས་ སྟོད་ ན་ གནས་པ་ གྲུབ་པ་ སྟེ་ རྨོངས་ ཤིང་ ལྕི་ འཐིབ་ "
"སོགས་ མགོ་བོ་ ལ␣ས་ འབྱུང་བ་ མཐོང་བ␣འི་ ཕྱིར་ རོ །"
msgstr ""

msgctxt "9213b32781f8470eb96a74e6c5f94113"
msgid ""
"མེ␣འམ་ ཉི་མ␣འི་ ཁམས་ རྒྱུ་བ␣འི་ རྩ་ གཅིག་ བར་ དུ་ ཟུག་ ཅིང་ དེ་ ལ་ བརྟེན་ "
"ནས་ སྲོག་པ་ རྩ་ ཆགས་ ཤིད་ ཁྲག་མཁྲིས་ ཚ་བ␣འི་ རྒྱུ␣ར་ གྱུར་པ␣འི་ ཞེ་སྡང་ ཡང་ "
"སྲོག་རྩ␣འི་ ཁྲག་ ལ་ བརྟེན་ ནས་ གནས་པ་ སྟ །"
msgstr ""

msgctxt "a7a47bfd666e4440ac87f532227c2c0d"
msgid ""
"རྒྱུ་ དེ་ལས་ མཁྲིས་པ་ ཚ་བ␣འི་ རང་བཞིན་ སྐྱེད་པ␣ས་ བར་ ན་ གནས་པ␣ས་ རྒྱ་འབྲས་ "
"རྟེན་ བཅས་ བར་ ན་ གནས་པ་ གྲུབ་པ་ སྟེ་ ཁོང་ཁྲོ་ དྲག་པོ་ སོགས་ ལྡང་བ␣འི་ ཚེ་ "
"ཤས་ ཆེ␣ར་ ལུས་ ཀྱི་ བར་ སྙིང་ ཐད་ ནས་ འབྱུང་བ␣འི་ ཕྱིར་ རོ །"
msgstr ""

msgctxt "c7fde95ad2a24b4d940b06c75cf670bf"
msgid ""
"ཟུང་འཇུག་ རླུང་ གི་ ཁམས་ རྒྱུ་བ␣འི་ རྩ་ གཅིག་ ཐུར་ དུ་ ཟུག་པ␣ས་ དེ␣འི་ "
"རྩེ་མོ་ ཆགས་ཚུལ་ ལེའུ་ ལ␣ས །  དྲུག་པ་ ལྟེ་བ་ ལ་ བརྟེན་ སྲོག་རྩ་ ཆགས་ ཞེས་པ་ "
"དེ་ཉིད་ ཡིན་ ཅིང་ །"
msgstr ""

msgctxt "38ce6432c8534b8b8ab2c5cb38b21bdf"
msgid ""
"དེ␣འི་ གཤམ་ སྣེ་ མན་ རྒྱུད་ བྱང་ཁོག་ རྨ་ གསོ་བ␣འི་ སྐབས །  རྩ་ ཡི་ རྒྱལ་པོ་ "
"སྲོག་རྩ་ བཅོས་པ་ ནི །"
msgstr ""

msgctxt "86d50523696b4bed9de8c1b8ab67ee61"
msgid ""
"ལྟེ་འོག་ སོ␣ར་ བཞི་ མིག་དམར་ ལ་སོགས་ དང་ ། ། ཞེས་ སོགས་ དེ་ ལ་ བརྟེན་པ␣འི་ "
"གསང་བ་ ཆགས་ ཤིང་ །"
msgstr ""

msgctxt "2d15e83aeeeb4984a5b3874ca54cd3a1"
msgid ""
"ཟུང་འཇུག་ རླུང་ གི་ རྒྱུ␣ར་ གྱུར་པ␣འི་ འདོད་ཆགས་ ནི་ ཕོ་མོ␣འི་ གསང་བ␣ར་ "
"གནས་པ་ སྟེ །"
msgstr ""

msgctxt "5f9d78157fe7432989909135d1bc0c91"
msgid ""
"དེ་ལས་ ཚ་གྲང་ གཉིས་ ཀྱི་ རང་བཞིན་ རླུང་ སྐྱེད་ ནས་ སྨད་ ན་ གནས་པ་ ཡིན་པ␣ས་ "
"རྒྱུ་འབྲས་ རྟེན་ བཅས་ སྨད་ ན་ གནས་པ་ གྲུབ་པ་ སྟེ །  འདོད་སྲེད་ ཤས་ ཆེ␣ར་ "
"སྨད་ ནས་ འབྱུང་བ་ མཐོང་བ␣འི་ ཕྱིར་ རོ ། །"
msgstr ""

msgctxt "f66d26d0ed6244bfabc7c4edae632258"
msgid ""
"རྒྱུད་ བཞི་  བདུད་རྩི་ སྙིང་པོ་ ཡན་ལག་ བརྒྱད་པ་ གསང་བ་ མན་ངག་ གི་ རྒྱུད་ "
"ཅེས་བྱ་བ་ བཞུགས་ སོ་"
msgstr ""

msgctxt "317720ddff7b40ab8f4ed3944d3f3e3b"
msgid "། སྲིད་པ␣འི་ རྩ་བོ་ ཆེ་ ནི་ རྣམ་ བཞི་ སྟེ །"
msgstr ""

msgctxt "67c643e09d8f457b9544fff22bb453e7"
msgid ""
"། དབང་པོ་ ཡུལ་ ལ་ འཆར་བ␣ར་ བྱེད་པ␣འི་ རྩ ། ། ཀླད་པ་ ལ་ སྲིད་ རྩ་ཕྲན་ ལྔ་ "
"བརྒྱ␣ས་ བསྐོར །"
msgstr ""

msgctxt "9c18eb0ada034e898841113adf55c2c8"
msgid ""
"། དྲན་པ␣འི་ དབང་པོ་ གསལ་བ␣ར་ བྱེད་པ␣འི་ རྩ ། ། སྙིང་ ལ་ སྲིད་ ཅིང་ རྩ་ཕྲན་ "
"ལྔ་ བརྒྱ␣ས་ བསྐོར །"
msgstr ""

msgctxt "e31921239b9f4e90954986b9faf46159"
msgid ""
"། ལུས་ ཀྱི་ ཕུང་པོ་ ཆགས་པ␣ར་ བྱེད་པ␣འི་ རྩ ། ། ལྟེ་བ་ ལ་ སྲིད་ རྩ་ཕྲན་ ལྔ་ "
"བརྒྱ␣ས་ བསྐོར །"
msgstr ""

msgctxt "5c990d55c46e43e4b29958ca28790b8d"
msgid "། སྟེང་འོག་ ཐད་ཀ␣ར་ ལུས་ ཀུན་ རྗེས་སུ་ འཛིན །"
msgstr ""

msgctxt "c21d85d24d2d4e12917ae75fadb56c91"
msgid ""
"གསོ་བ་ རིག་པ␣འི་ བསྟན་བཅོས་ སྨན་བླ␣འི་ དགོངས་ རྒྱན་ རྒྱུད་ བཞི␣འི་ གསལ་བྱེད་"
" བཻདུརྱ་ སྔོན་པོ␣འི་ མལླིཀ་ ཞེས་ བྱ་བ་ བཞུགས་ སོ །།"
msgstr ""

msgctxt "49cacf8c151344d09c8a4b8c45959bd5"
msgid ""
"། ལུས་ འདི་ ལ་ སྲིད་པ␣འི་ རྩ་བོ་ ཆེ་ཞེ␣ས་པ་ མིང་ གིས་ བཏགས་པ་ ནི་ རྣམ་པ་ "
"བཞི་ འམ་ ལྔ་ ཡོད་ དེ །  གང་ཞེ་ན་ མིག་ ལ་སོགས་པ␣འི་ དབང་པོ་ རྣམས་ རང་རང་ གི་ "
"ཡུལ་ གཟུགས་ ལ་སོགས་པ་ རྣམས་ འཆར་བ␣ར་ བྱེད་པ␣འི་ རྩ་ ཉི་ཤུ་ རྩ་ བཞི་ ནི་ "
"ཡན་ལག་ གི་ རླུང་ ལྔ␣འི་ རྟེན་ དུ་ ཀླད་པ␣ར་ འཁྱིལ་བ་ ཞེས་པ␣འི་ རྩ་ ལ་ "
"རྒྱལ་པོ་ ལྟ་བུ␣ར་ སྲིད་ ཅིང་ འཁོར་ རྩ་ཕྲན་ ལྔ་ བརྒྱ␣ས་ བསྐོར་ ཏེ་ གནས་པ་ "
"ཡིན་ ནོ ། ། མགྲིན་པ་ ལ་ སྲིད་ རྩ་ཕྲན་ ལྔ་ བརྒྱ␣ས་ བསྐོར ། ། ཞེས་ དོན་ གྱིས་ "
"ཐོབ་ ཅིང་ གོ་ ནུས་པ་ ཡིན་ ནོ །"
msgstr ""

msgctxt "575b2021181f4246a2ad20154d455d2d"
msgid ""
"རང་རང་ གི་ ལུས་ ཀྱི་ ཕུང་པོ་ འདི་ཉིད་ ཐོག་མཐའ་ བར་ གསུམ་ དུ་ ཆགས་ ཤིང་འཕེལ་ "
"བར་ བྱེད་པ␣འི་ རྩ་ ཉི་ཤུ་ རྩ་ བཞི་ ནི་ ས་བོན་ འཕེལ་ འཛིན་བྱེད་པ␣འི་ རྟེན་ "
"དུ་ ལྟེ་བ␣འི་ དབུས་ ཀྱི་ བསྟེན་པ་ ཞེས་པ␣འི་ རྩ་ ལ་ རྒྱལ་པོ་ ལྟ་བུ␣ར་ སྲིད་ "
"ཅིང་ འཁོར་ རྩ་ཕྲན་ ལྔ་ བརྒྱ␣ས་ བསྐོར་ ཏེ་ གནས་པ་ ཡིན་ ནོ །"
msgstr ""

msgctxt "f3142f9851094dbc990b5c20cceb8b82"
msgid ""
"། བུ་ཚ་ རིགས་ བརྒྱུད་ མི་ ཉམས་པ␣ར་ སྐྱེད་ ཅིང་ འཕེལ་བ␣ར་ བྱེད་པ␣འི་ རྩ་ "
"ཉི་ཤུ་ རྩ་ བཞི་ ནི་ བདེ་བ␣འི་ ཁམས་ ཀྱི་ རྟེན་ དུ་ མཚན་མ␣ར་ མཚན་ཉིད་ ཅན་ "
"ཞེས་པ␣འི་ རྩ་ ལ་ རྒྱལ་པོ་ ལྟ་བུ␣ར་ སྲིད་ ཅིང་ འཁོར་ རྩ་ཕྲན་ ལྔ་ བརྒྱ␣ས་ "
"བསྐོར་ ཏེ་ གནས་པ་ ཡིན་ ནོ །"
msgstr ""

msgctxt "87198cd3958240c4becfffc33c32b8a6"
msgid ""
"། དེ་ལྟར་ བཤད་པ་ རྣམས་ རགས་པ␣འི་ དབང་ དུ་ བྱས་ མོད །  ཞིབ་ ཏུ་ རྩ་ འདབ་ ཀྱི་"
" གྲངས་ དང་ མིང་ རྣམས་ དུས་ ཀྱི་ འཁོར་ལོ་ ལ་སོགས་པ␣འི་ རྒྱུད་སྡེ་ རྣམས་ དང་ "
"མཐུན་པ␣ར་ འཆད་ དུ་ཡོད་ དོ ། ། དེ་ལྟར་ བཤད་པ␣འི་ འཁོར་ལོ་ མགྲིན་པ་ དང་ བཅས་པ་"
" ལྔ␣འི་ རྩ་ ཕྲ་རགས་པ་ རྣམས་ ཀྱི་ རང་རང་ གི་ ལྟེ་བ␣འི་ སྟེང་ དང་ འོག་ དང་ "
"ཐད་ཀ␣ར་ ཁམས་ ཐམས་ཅད་ ཀྱི་ བྱ་བ་ ཚུལ་བཞིན་ དུ་ བྱེད་ ཅིང་ ཡོངས་སུ་ "
"འབྲེལ་བ␣འི་ དོན་ གྱིས་ ན །"
msgstr ""

msgctxt "e69333cc61364d8db309e882c1aff0fc"
msgid ""
"ལུས་ ཀྱི་ ཆ་ཤས་ ནད་ ཟུངས་ དྲི་མ་ སོགས་ ཇི་སྙེད་པ་ ཀུན་ ཀྱང་ དུས་ མ་ཡིན་པ␣ར་ "
"མི་ གཏོང་བ␣ར་ རྗེས་སུ་ འཛིན་པ་ ཡིན་ ནོ །"
msgstr ""

msgctxt "3d6a54948fc4485eaf730fb2f9ba77e6"
msgid ""
"རྒྱུད་ བཞི་  བདུད་རྩི་ སྙིང་པོ་ ཡན་ལག་ བརྒྱད་པ་ གསང་བ་ མན་ངག་ གི་ རྒྱུད་ "
"ཅེས་བྱ་བ་ བཞུགས་ སོ་"
msgstr ""

msgctxt "faf7bb4bfe4643f6b540ca942c7c5361"
msgid ""
"། ཀླད་པ་ རྩ་ ཡི་ རྒྱ་མཚོ་ ཆེན་པོ་ ལ␣ས ། ། རྩ་བ་ ལྟ་བུ␣ར་ ཐུར་ དུ་ ཟུག་པ་ ཡི "
"།"
